generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model solicitacao {
  id             Int       @id @default(autoincrement())
  requester      String
  subtitle       String
  description    String
  ticket         String    @unique
  typeproblem    String
  createdAt      DateTime  @default(now())
  finishedAt     DateTime?
  finishedByUser String?
  updatedAt      DateTime  @updatedAt
}

model novos_produtos {
  id                  Int       @id @default(autoincrement())
  requester           String
  ticket              String    @unique
  typeproblem         String
  nameClient          String
  portUrgency         String?
  nameProduct         String
  quantityAndDelivery String
  observation         String
  createdAt           DateTime  @default(now())
  finishedAt          DateTime?
  finishedByUser      String?
  updatedAt           DateTime  @updatedAt
}
